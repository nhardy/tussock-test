{
  "name": "web-scaffold",
  "version": "0.0.0",
  "description": "An example web scaffold",
  "main": "server.js",
  "scripts": {
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm test -- --reporter min",
    "lint": "eslint ./src ./test ./build",
    "lint:fix": "eslint --fix ./src ./test ./build",
    "flow": "flow",
    "flowtypes": "flow-typed install --overwrite",
    "dev": "node ./build/index.js dev",
    "prod": "node ./build/index.js prod",
    "start": "node server.js",
    "test": "mocha",
    "package": "node ./build/index.js package",
    "deploy": "node ./build/index.js deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/web-scaffold.git"
  },
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/web-scaffold/issues"
  },
  "homepage": "https://github.com/nhardy/web-scaffold#readme",
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "build",
      "coverage",
      "dist",
      "flow",
      "flow-typed",
      "node_modules",
      "test",
      "server.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "all": true
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "archiver": "^2.1.0",
    "autoprefixer": "^7.1.5",
    "azur": "^0.0.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "buffer-loader": "0.0.1",
    "cache-loader": "^1.1.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-nhardy": "^0.3.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.0",
    "glob": "^7.1.2",
    "httpolyglot": "^0.1.2",
    "jsdom": "^11.3.0",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "node-hook": "^1.0.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "postcss-loader": "^2.0.8",
    "proxyquire": "^1.8.0",
    "raw-loader": "^0.5.1",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.0.0",
    "selfsigned": "^1.10.1",
    "sinon": "^4.0.1",
    "string-hash": "^1.1.3",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svgo-loader": "^1.2.1",
    "text-loader": "^0.0.1",
    "thread-loader": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^9.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.4",
    "nodemailer": "^4.2.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-display-name": "^0.2.3",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.3",
    "redux": "^3.7.2",
